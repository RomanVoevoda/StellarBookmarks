{
  "name": "Stellar Bookmarks",
  "description": "A convenient manager for managing bookmarks, in an eye-pleasing space style.",
  "version": "1.0",
  "author": "karamount22@gmail.com",
  "manifest_version": 3,
  "icons": {
    "16": "icons/saturn-16x16px.png",
    "32": "icons/saturn-32x32px.png",
    "48": "icons/saturn-48x48px.png",
    "128": "icons/saturn-128x128px.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Click to open panel"
  },
  "commands": {},
  "permissions": [
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "popup.html"
  },
  "background":{
    "service_worker": "scripts/service-worker.js",
    "type": "module"
  }
}